    const currentPage = getCurrentPage();
    if (currentPage > 1) {
        // Có thể chỉnh sửa tùy theo số trang tối đa của bạn
        document.querySelector(".prev-page-button").addEventListener("click", () => {
            changePage(currentPage - 1);
        });
    }
    if (currentPage <3)
    {
    // Có thể chỉnh sửa tùy theo số trang tối đa của bạn
    document.querySelector(".next-page-button").addEventListener("click", () => {
        changePage(currentPage + 1);
    });
    }
});


const itemStates = [];

function toggleCheckmark1(element, index) {
      const removeButtons = document.querySelectorAll(".Products__active-facets-button-inner-twcss-text-body-12-twcss-font-fenomen");
    const lis = document.querySelectorAll(".list-menu__item");

    lis.forEach((li, index) => {
        li.addEventListener("click", () => {
            itemStates[index] = !itemStates[index];
            const correspondingRemoveButton = removeButtons[index];
            correspondingRemoveButton.style.display = itemStates[index] ? "block" : "none";
        });
    });

    removeButtons.forEach((removeButton, index) => {
        removeButton.addEventListener("click", (event) => {
            event.preventDefault();
            removeButton.style.display = "none";
            itemStates[index] = false;
            // Bổ sung dòng sau để bỏ check khi ẩn nút "Remove filter"
            lis[index].classList.remove("checked");
        });
    });
}



