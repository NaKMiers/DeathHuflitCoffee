<script>
    function toggleModal() {
        const modal = document.getElementById("Facet-1-template--14567496974391__product-grid");
        modal.classList.toggle("Facet-1-template--14567496974391__product-grid-show");
    }

    // Thêm sự kiện click cho toàn bộ tài liệu (document)
    document.addEventListener("click", function (event) {
        const modal = document.getElementById("Facet-1-template--14567496974391__product-grid");

        // Kiểm tra xem sự kiện click có xảy ra bên ngoài modal và không phải là nút mở modal không
        if (!modal.contains(event.target) && !event.target.classList.contains("Products__ads-button") && !event.target.classList.contains("Products__twcss-text-body-16-twcss-font-fenomen")) {
            // Nếu click bên ngoài modal và không phải là nút mở modal, đóng modal
            modal.classList.remove("Facet-1-template--14567496974391__product-grid-show");
        }
    });

    // Sự kiện click trên chữ "Type" cũng gọi hàm toggleModal
    document.querySelector(".Products__ads-button").addEventListener("click", toggleModal);
    document.querySelector(".Products__twcss-text-body-16-twcss-font-fenomen").addEventListener("click", toggleModal);
    // Lấy thẻ chứa chữ "Type" và biểu tượng SVG
    const typeButton = document.querySelector(".Products__ads-button");
    // Thêm sự kiện click cho biểu tượng SVG
    const typeIcon = typeButton.querySelector("svg.Products__icon-icon-caret");
    typeIcon.addEventListener("click", function (event) {
        event.stopPropagation(); // Ngăn sự kiện click từ việc lan truyền lên thẻ chứa "Type"
        toggleModal(); // Gọi hàm toggleModal để mở hoặc đóng modal
    });

    let originalProductCount = 45; // Số lượng sản phẩm ban đầu
    let selectedProductCount = 0; // Số lượng sản phẩm đã chọn
    let productCountSpan = document.getElementById("ProductCountDesktop");

    function toggleCheckmark(element) {
        const checkbox = element.querySelector('input[type="checkbox"]');
        const checkmark = element.querySelector(".Products__icon-icon-checkmark");
        const labelText = element.querySelector(".visually-hidden").textContent;
        const numberMatch = labelText.match(/\d+/);

        if (checkbox.checked) {
            checkbox.checked = false; // Bỏ chọn checkbox
            checkmark.style.display = "none"; // Ẩn dấu tích
            selectedProductCount -= parseInt(numberMatch[0]);
        } else {
            checkbox.checked = true; // Chọn checkbox
            checkmark.style.display = "inline-block"; // Hiển thị dấu tích
            selectedProductCount += parseInt(numberMatch[0]);
        }

        updateProductCount();
    }

    // Hàm cập nhật số lượng sản phẩm đã chọn
    function updateProductCount() {
        if (selectedProductCount === 0) {
            productCountSpan.textContent = `${originalProductCount} products`;
        } else {
            productCountSpan.textContent = `${selectedProductCount / 2} of ${originalProductCount} products`;
        }
    }

    // Gọi hàm cập nhật số lượng sản phẩm ban đầu
    updateProductCount();
    const itemsList = document.querySelector('.items-list');
    const pageLinks = document.querySelectorAll('.page-link');
    const prevPageButton = document.querySelector('.prev-page-button'); // Thêm nút "Prev"

    const itemsPerPage = 24; // Số lượng mục trên mỗi trang
    let currentPage = 1; // Trang hiện tại

    // Hiển thị các mục cho trang cụ thể
    function displayPage(page) {
        const startIndex = (page - 1) * itemsPerPage;
        const endIndex = startIndex + itemsPerPage;

        const items = document.querySelectorAll('.items-list li');

        items.forEach((item, index) => {
            if (index >= startIndex && index < endIndex) {
                item.style.display = 'block';
            } else {
                item.style.display = 'none';
            }
        });
    }

    // Hàm chuyển đến trang
    function changePage(page) {
        currentPage = page;
        displayPage(currentPage);
        updatePaginationButtons(); // Cập nhật trạng thái của nút "Prev" và "Next"
    }

    // Hàm chuyển đến trang kế tiếp
    function nextPage() {
        if (currentPage < pageLinks.length - 2) {
            changePage(currentPage + 1);
        }
    }

    // Hàm chuyển đến trang trước đó
    function prevPage() {
        if (currentPage > 1) {
            changePage(currentPage - 1);
        }
    }

    // Tạo 72 mục và thêm chúng vào danh sách
    for (let i = 1; i <= 72; i++) {
        const listItem = document.createElement('li');
        listItem.textContent = `Mục ${i}`;
        itemsList.appendChild(listItem);
    }

    // Ban đầu hiển thị trang đầu tiên và cập nhật trạng thái của nút "Prev" và "Next"
    displayPage(currentPage);
    updatePaginationButtons();

    // Hàm cập nhật trạng thái của nút "Prev" và "Next"
    function updatePaginationButtons() {
        if (currentPage === 1) {
            prevPageButton.style.display = 'none';
        } else {
            prevPageButton.style.display = 'block';
        }

        if (currentPage === pageLinks.length) {
            pageLinks[pageLinks.length - 1].classList.add('active');
        } else {
            pageLinks.forEach((link) => {
                link.classList.remove('active');
            });

            pageLinks[currentPage - 1].classList.add('active');
        }
    }

</script>
