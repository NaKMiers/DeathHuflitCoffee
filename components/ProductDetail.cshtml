                                </svg></span></div>
                        <div class="paragraph" itemprop="ratingValue">4.7</div>
                        <div class="paragraph" aria-hidden="true">
                            <meta itemprop="reviewCount" content="1883">
                            @* <div class="paragraph">(1883)</div> *@
                        </div>

                    </div>
                </div>

                <!-- phần thân -->
                <div class="KproductDetail__main__text__infoofcoffee w-100">
                    <h4>@Model.Subtitle</h4>
                    <p>
                        @Model.Description
                    </p>

                    <!-- size -->
                    <div class="KproductDetail__main__text__size__all__div9" style="display:flex">

                        <div class="KproductDetail__main__text__size__all">

                            <div class="KproductDetail__main__text__size_header" style="text-align:left;">
                                <p style="text-align:left;"><strong>Size</Strong></p>
                            </div>
                            <div class="KproductDetail__main__text__size">
                                <div id="KproductDetail__main__text__size__choice__text1">
                                    <p class="KproductDetail_Size_main_p">
                                        <img class="KproductDetail_Size_div92__img"
                                            src="data:image/jpeg;base64,">
                                        &nbsp; 5 bags &nbsp; &nbsp; $84.99
                                    </p>
                                </div>
                            </div>


                            <div class='KproductDetail__main__text__size__choice'>
                                <div class='KproductDetail__main__text__size__choice__text'>
                                    <p class="KproductDetail_Size_p">
                                        <img class="KproductDetail_Size_div92__img"
                                            src="data:image/jpeg;base64,">
                                        &nbsp; 5 bags &nbsp; &nbsp; $84.99
                                    </p>
                                </div>
                                <div class='KproductDetail__main__text__size__choice__text'>
                                    <p class="KproductDetail_Size_p">
                                        <img class="KproductDetail_Size_div92__img"
                                            src="data:image/jpeg;base64,">
                                        &nbsp; 2 bags &nbsp; &nbsp; $39.89
                                    </p>
                                </div>
                                <div class='KproductDetail__main__text__size__choice__text'>
                                    <p class="KproductDetail_Size_p">
                                        <img class="KproductDetail_Size_div92__img"
                                            src="data:image/jpeg;base64,">
                                        &nbsp; 1 bags &nbsp; &nbsp; $19.99
                                    </p>
                                </div>
                            </div>

                        </div>


                        <!-- type -->
                        <div class="KproductDetail__main__text__size2__all">
                            <div class="KproductDetail__main__text__size2_header">
                                <h4 class="KproductDetail__main__text__size_42"><strong>Type</strong></h4>
                            </div>

                            <div class="KproductDetail__main__text__size2">
                                <div class="KproductDetail__main__text__size__mainn2">
                                    <p class="KproductDetail_Size_div92">
                                        <img src="data:image/jpeg;base64,"
                                            width="30px" ; height="30px">
                                        GROUND
                                    </p>
                                </div>
                            </div>

                            <div class="KproductDetail__main__text__size__choice2">
                                <div class="KproductDetail__main__text__size__choice__p2">
                                    <p class="KproductDetail_Size_p2">
                                        <img src="data:image/jpeg;base64,"
                                            width="30px" ; height="30px">
                                        GROUND
                                    </p>
                                </div>
                                <div class="KproductDetail__main__text__size__choice__p2">
                                    <p class="KproductDetail_Size_p2">
                                        <img src="data:image/jpeg;base64,"
                                            width="30px" height="30px">
                                        WHOLE Bean
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- add to cart -->
                    <form action="/cart/add/@ViewBag.UserId" method="post"
                        class="cart-KproductDetail__main__text__add-item">
                        <input class="form-control" name="productId" type="hidden" value="@Model.Id">
                        <input type="hidden" name="size"
                            value="@Model.Sizes.FirstOrDefault()?.Label - @Model.Sizes.FirstOrDefault()?.Price"
                            asp-for="Size">
                        <input type="hidden" name="quantity" value="1" asp-for="Quantity">

                        <button class="KproductDetail__main__text__add_button">ADD TO CART</button>
                    </form>

                    <!-- flavor profile -->
                    <div class="KproductDetail__main__text_FlavorProfile">
                        <br>
                        <hr class="KproductDetail__main__text_FlavorProfile__hr">
                        <details style="display: flex; align-items: center;">
                            <summary
                                style="flex: 1; display: flex; align-items: center; color: white; background-color: black;padding-bottom:10px">
                                <div class="summary__title__twcss__uppercase__twcss__grow">
                                    <h2 class="summary__title__twcss__uppercase__twcss__grow__FlavorProfile fs-5">Flavor
                                        Profile </h2>
                                </div>
                                <svg width="12" height="7" viewBox="0 0 12 7" fill="none">
                                    <path d="M1.27344 1L6.00071 5L10.728 1" stroke="white" stroke-width="2"></path>
                                </svg>
                            </summary>
                            <div class="metafield__rich_text_field">
                                @if (Model.FlavorProfiles != null && Model.FlavorProfiles.Count > 0)
                                {
                                    @foreach (var item in Model.FlavorProfiles)
                                    {
                                        <p><strong>@item.Label:</strong> @item.Text</p>
                                    }
                                }
                            </div>

                        </details>

                        <!-- atribute -->
                        <div class="KproductDetail__main__text_Atribute">
                            <hr class="KproductDetail__main__text_FlavorProfile__hr">

                            <details class="KproductDetail__main__text_Atribute__detail">
                                <summary class="KproductDetail__main__text_Atribute__summary">
                                    <div class="summary__title__twcss__uppercase__twcss__grow">
                                        <h2 class="summary__title__twcss__uppercase__twcss__grow__h2">Attributes </h2>
                                    </div>
                                    <svg width="12" height="7" viewBox="0 0 12 7" fill="none">
                                        <path d="M1.27344 1L6.00071 5L10.728 1" stroke="white" stroke-width="2"></path>
                                    </svg>
                                </summary>
                                <div class="KproductDetail__main__text_Atribute__cacthanh">

                                    @if (Model.Attributes != null && Model.Attributes.Count > 0)
                                    {
                                        @foreach (var item in Model.Attributes)
                                        {
                                            <div class="metafield-rich_text_summary_div1">
                                                <h6>@item.Label</h6>
                                                <Div style="background-color: gray;display: flex; height:5px;">
                                                    <div style="background-color: orangered; width: @(item.Value + "%");">

                                                    </div>
                                                    <div class="metafield-rich_text_summary_div1_cirle">

                                                    </div>

                                                </Div>
                                                <div style="display:flex; width: 150%;">
                                                    <p style="text-align:left; width: 33%;">@item.MinLabel</p>
                                                    <p style="text-align:right; width: 33%;">@item.MaxLabel</p>
                                                </div>
                                            </div>
                                        }
                                    }
                            </details>
                        </div>
                    </div>

                    <div class="KproductDetail__main__text_FlavorProfile">
                        <hr class="KproductDetail__main__text_FlavorProfile__hr">
                        <details style="display: flex; align-items: center;">
                            <summary
                                style="flex: 1; display: flex; align-items: center; color: white; background-color: black;padding-bottom:10px">
                                <div class="summary__title__twcss__uppercase__twcss__grow">
                                    <h2 class="summary__title__twcss__uppercase__twcss__grow__FlavorProfile fs-5">Detail
                                    </h2>
                                </div>
                                <svg width="12" height="7" viewBox="0 0 12 7" fill="none">
                                    <path d="M1.27344 1L6.00071 5L10.728 1" stroke="white" stroke-width="2"></path>
                                </svg>
                            </summary>
                            <div class="metafield__rich_text_field">
                                @if (Model.Details != null && Model.Details.Count > 0)
                                {
                                    @foreach (var item in Model.Details)
                                    {
                                        <p><strong>@item.Label:</strong> @item.Text</p>
                                    }
                                }
                            </div>
                        </details>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>