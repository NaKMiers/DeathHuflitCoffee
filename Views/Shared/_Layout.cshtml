<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DeathWishCoffee</title>
    <link rel="stylesheet" href="~/fonts/Fenomen-Sans-SemiBold.otf">
    <link rel="stylesheet" href="~/fonts/Revans-Bold.otf">
    <link rel="stylesheet" href="~/fonts/Fenomen-Sans-Bold.woff2">
    <link rel="stylesheet" href="~/fonts/Fenomen-Sans-Book.woff2">
    <link rel="stylesheet" href="~/fonts/KFOlCnqEu92Fr1MmEU9fBBc4.woff2">
    <link rel="stylesheet" href="~/fonts/proximanova-regular.woff2">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/DeathWishCoffee.styles.css" asp-append-version="true" />

    <link rel="stylesheet" href="~/css/components/header.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/review.css" asp-append-version="true" />
</head>

<body>
    @Html.Partial("../../components/Header.cshtml")

    <div class="container" style="height: 200vh;">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - DeathWishCoffee - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://kit.fontawesome.com/58516b09e7.js" crossorigin="anonymous"></script>

    <script src="~/js/components/header.js" type="module"></script>
    <script src="~/js/components/review.js" type="module"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
